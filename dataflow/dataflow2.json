{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Stage_Source",
						"type": "DatasetReference"
					},
					"name": "sourceStage"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          INVOICEID as decimal(30,0),",
				"          CUSTOMERID as decimal(30,0),",
				"          INVOICEDATE as date,",
				"          SHIPTO as string,",
				"          SHIPTOCITY as string,",
				"          SHIPTOCOUNTRY as string,",
				"          SALESPERSONID as string,",
				"          SALESPERSONNAME as string,",
				"          TOTALPRODUCTCOUNT as decimal(10,4),",
				"          TOTAL as decimal(38,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> sourceStage"
			]
		}
	}
}